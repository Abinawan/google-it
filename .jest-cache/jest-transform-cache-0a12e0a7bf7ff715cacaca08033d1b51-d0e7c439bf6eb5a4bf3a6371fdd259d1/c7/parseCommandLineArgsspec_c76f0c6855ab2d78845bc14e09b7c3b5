"use strict";

var _commandLineArgs = _interopRequireDefault(require("command-line-args"));

var _parseCommandLineArgs = _interopRequireDefault(require("../src/parseCommandLineArgs"));

function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

describe('parseCommandLineArgs', function () {
  it('returns object with only "query" field when argv.length is 3', function () {
    var result = (0, _parseCommandLineArgs.default)(['node', '/some/random/path', 'my query']);
    expect(result).toEqual({
      query: 'my query'
    });
  });
  it('calls commandLineArgs when argv.length is something other than 3', function () {
    jest.mock('command-line-args', function () {});
    _commandLineArgs.default = (jest.fn(function () {}), function () {
      throw new Error('"' + "commandLineArgs" + '" is read-only.');
    }());
    (0, _parseCommandLineArgs.default)(['node', '/some/random/path', 'my query', 'cinco de mayo']);
    expect(_commandLineArgs.default).toHaveBeenCalled();
  });
});
