      cov_1c1xru6na2.f[8]++;
      cov_1c1xru6na2.s[27]++;
      console.log(
      /* istanbul ignore next */
      "Html file saved to ".concat(htmlFileOutputPath));
    });
  } else
  /* istanbul ignore next */
  {
    cov_1c1xru6na2.b[9][1]++;
  }
};

