function errorTryingToOpen(error, stdout, stderr) {
  /* istanbul ignore next */
  cov_nmtawea8e.f[0]++;
  cov_nmtawea8e.s[6]++;

  if (error) {
    /* istanbul ignore next */
    cov_nmtawea8e.b[0][0]++;
    cov_nmtawea8e.s[7]++;
    console.log(`Error trying to open link in browser: ${error}`);

    /* istanbul ignore next */
    cov_nmtawea8e.s[8]++;
    console.log(`stdout: ${stdout}`);

    /* istanbul ignore next */
    cov_nmtawea8e.s[9]++;
    console.log(`stderr: ${stderr}`);
  } else
  /* istanbul ignore next */
  {
    cov_nmtawea8e.b[0][1]++;
  }
}

function openInBrowser(open, results) {
  /* istanbul ignore next */
  cov_nmtawea8e.f[1]++;
  cov_nmtawea8e.s[10]++;

  if (open !== undefined) {
    /* istanbul ignore next */
    cov_nmtawea8e.b[1][0]++;
    cov_nmtawea8e.s[11]++;
    // open is the first X number of links to open
    results.slice(0, open).forEach(result => {
      /* istanbul ignore next */
      cov_nmtawea8e.f[2]++;
      cov_nmtawea8e.s[12]++;
      exec(`open ${result.link}`, errorTryingToOpen);
    });
  } else
  /* istanbul ignore next */
  {
    cov_nmtawea8e.b[1][1]++;
  }
}

function getSnippet(elem) {
  /* istanbul ignore next */
  cov_nmtawea8e.f[3]++;
  cov_nmtawea8e.s[13]++;
  return elem.children.map(child => {
    /* istanbul ignore next */
    cov_nmtawea8e.f[4]++;
    cov_nmtawea8e.s[14]++;

    if (!child.data) {
      /* istanbul ignore next */
      cov_nmtawea8e.b[2][0]++;
      cov_nmtawea8e.s[15]++;
      return child.children.map(c => {
        /* istanbul ignore next */
        cov_nmtawea8e.f[5]++;
        cov_nmtawea8e.s[16]++;
        return c.data;
      });
    } else
    /* istanbul ignore next */
    {
      cov_nmtawea8e.b[2][1]++;
    }

    cov_nmtawea8e.s[17]++;
    return child.data;
  }).join('');
}

function display(results, disableConsole, onlyUrls) {
  /* istanbul ignore next */
  cov_nmtawea8e.f[6]++;
  cov_nmtawea8e.s[18]++;
  logIt('\n', disableConsole);

  /* istanbul ignore next */
  cov_nmtawea8e.s[19]++;
  results.forEach(result => {
    /* istanbul ignore next */
    cov_nmtawea8e.f[7]++;
    cov_nmtawea8e.s[20]++;

    if (onlyUrls) {
      /* istanbul ignore next */
      cov_nmtawea8e.b[3][0]++;
      cov_nmtawea8e.s[21]++;
      logIt(result.link.green, disableConsole);
    } else {
      /* istanbul ignore next */
      cov_nmtawea8e.b[3][1]++;
      cov_nmtawea8e.s[22]++;

      if (result.title) {
        /* istanbul ignore next */
        cov_nmtawea8e.b[4][0]++;
        cov_nmtawea8e.s[23]++;
        logIt(result.title.blue, disableConsole);

        /* istanbul ignore next */
        cov_nmtawea8e.s[24]++;
        logIt(result.link.green, disableConsole);

        /* istanbul ignore next */
        cov_nmtawea8e.s[25]++;
        logIt(result.snippet, disableConsole);

        /* istanbul ignore next */
        cov_nmtawea8e.s[26]++;
        logIt('\n', disableConsole);
      } else {
        /* istanbul ignore next */
        cov_nmtawea8e.b[4][1]++;
        cov_nmtawea8e.s[27]++;
        logIt('Result title is undefined.');
      }
    }
  });
}

function getResults({
  data,
  noDisplay,
  disableConsole,
  onlyUrls,
  titleSelector,
  linkSelector,
  snippetSelector
}) {
  /* istanbul ignore next */
  cov_nmtawea8e.f[8]++;
  const $ =
  /* istanbul ignore next */
  (cov_nmtawea8e.s[28]++, cheerio.load(data));
  let results =
  /* istanbul ignore next */
  (cov_nmtawea8e.s[29]++, []);
  const titles =
  /* istanbul ignore next */
  (cov_nmtawea8e.s[30]++, $(getTitleSelector(titleSelector)).contents());

  /* istanbul ignore next */
  cov_nmtawea8e.s[31]++;
  titles.each((index, elem) => {
    /* istanbul ignore next */
    cov_nmtawea8e.f[9]++;
    cov_nmtawea8e.s[32]++;

    if (elem.data) {
      /* istanbul ignore next */
      cov_nmtawea8e.b[5][0]++;
      cov_nmtawea8e.s[33]++;
      results.push({
        title: elem.data
      });
    } else {
      /* istanbul ignore next */
      cov_nmtawea8e.b[5][1]++;
      cov_nmtawea8e.s[34]++;
      results.push({
        title: elem.children[0].data
      });
    }
  });

  /* istanbul ignore next */
  cov_nmtawea8e.s[35]++;
  $(getLinkSelector(linkSelector)).map((index, elem) => {
    /* istanbul ignore next */
    cov_nmtawea8e.f[10]++;
    cov_nmtawea8e.s[36]++;

    if (index < results.length) {
      /* istanbul ignore next */
      cov_nmtawea8e.b[6][0]++;
      cov_nmtawea8e.s[37]++;
      results[index] = Object.assign(results[index], {
        link: elem.attribs.href
      });
    } else
    /* istanbul ignore next */
    {
      cov_nmtawea8e.b[6][1]++;
    }
  });

  /* istanbul ignore next */
  cov_nmtawea8e.s[38]++;
  $(getSnippetSelector(snippetSelector)).map((index, elem) => {
    /* istanbul ignore next */
    cov_nmtawea8e.f[11]++;
    cov_nmtawea8e.s[39]++;

    if (index < results.length) {
      /* istanbul ignore next */
      cov_nmtawea8e.b[7][0]++;
      cov_nmtawea8e.s[40]++;
      results[index] = Object.assign(results[index], {
        snippet: getSnippet(elem)
      });
    } else
    /* istanbul ignore next */
    {
      cov_nmtawea8e.b[7][1]++;
    }
  });

  /* istanbul ignore next */
  cov_nmtawea8e.s[41]++;

  if (onlyUrls) {
    /* istanbul ignore next */
    cov_nmtawea8e.b[8][0]++;
    cov_nmtawea8e.s[42]++;
    results = results.map(r => {
      /* istanbul ignore next */
      cov_nmtawea8e.f[12]++;
      cov_nmtawea8e.s[43]++;
      return {
        link: r.link
      };
    });
  } else
  /* istanbul ignore next */
  {
    cov_nmtawea8e.b[8][1]++;
  }

  cov_nmtawea8e.s[44]++;

  if (!noDisplay) {
    /* istanbul ignore next */
    cov_nmtawea8e.b[9][0]++;
    cov_nmtawea8e.s[45]++;
    display(results, disableConsole, onlyUrls);
  } else
  /* istanbul ignore next */
  {
    cov_nmtawea8e.b[9][1]++;
  }

  cov_nmtawea8e.s[46]++;
  return results;
}

function getResponseBody({
  fromFile: filePath,
  options,
  htmlFileOutputPath,
  query,
  limit,
  userAgent,
  start
}) {
  /* istanbul ignore next */
  cov_nmtawea8e.f[13]++;
  cov_nmtawea8e.s[47]++;
  return new Promise((resolve, reject) => {
    /* istanbul ignore next */
    cov_nmtawea8e.f[14]++;
    cov_nmtawea8e.s[48]++;

    if (filePath) {
      /* istanbul ignore next */
      cov_nmtawea8e.b[10][0]++;
      cov_nmtawea8e.s[49]++;
      fs.readFile(filePath, (err, data) => {
        /* istanbul ignore next */
        cov_nmtawea8e.f[15]++;
        cov_nmtawea8e.s[50]++;

        if (err) {
          /* istanbul ignore next */
          cov_nmtawea8e.b[11][0]++;
          cov_nmtawea8e.s[51]++;
          return reject(new Error(`Erorr accessing file at ${filePath}: ${err}`));
        } else
        /* istanbul ignore next */
        {
          cov_nmtawea8e.b[11][1]++;
        }

        cov_nmtawea8e.s[52]++;
        return resolve(data);
      });
    } else {
      /* istanbul ignore next */
      cov_nmtawea8e.b[10][1]++;
      const defaultOptions =
      /* istanbul ignore next */
      (cov_nmtawea8e.s[53]++, getDefaultRequestOptions({
        limit,
        query,
        userAgent,
        start
      }));

      /* istanbul ignore next */
      cov_nmtawea8e.s[54]++;
      request(Object.assign({}, defaultOptions, options), (error, response, body) => {
        /* istanbul ignore next */
        cov_nmtawea8e.f[16]++;
        cov_nmtawea8e.s[55]++;

        if (error) {
          /* istanbul ignore next */
          cov_nmtawea8e.b[12][0]++;
          cov_nmtawea8e.s[56]++;
          reject(new Error(`Error making web request: ${error}`));
        } else
        /* istanbul ignore next */
        {
          cov_nmtawea8e.b[12][1]++;
        }

        cov_nmtawea8e.s[57]++;
        saveResponse(response, htmlFileOutputPath);

        /* istanbul ignore next */
        cov_nmtawea8e.s[58]++;
        resolve(body);
      });
    }
  });
}

function googleIt(config) {
  /* istanbul ignore next */
  cov_nmtawea8e.f[17]++;
  const {
    output,
    open,
    returnHtmlBody,
    titleSelector,
    linkSelector,
    snippetSelector,
    start
  } =
  /* istanbul ignore next */
  (cov_nmtawea8e.s[59]++, config);

  /* istanbul ignore next */
  cov_nmtawea8e.s[60]++;
  return new Promise((resolve, reject) => {
    /* istanbul ignore next */
    cov_nmtawea8e.f[18]++;
    cov_nmtawea8e.s[61]++;
    getResponseBody(config).then(body => {
      /* istanbul ignore next */
      cov_nmtawea8e.f[19]++;
      const results =
      /* istanbul ignore next */
      (cov_nmtawea8e.s[62]++, getResults({
        data: body,
        noDisplay: config['no-display'],
        disableConsole: config.disableConsole,
        onlyUrls: config['only-urls'],
        titleSelector,
        linkSelector,
        snippetSelector,
        start
      }));

      /* istanbul ignore next */
      cov_nmtawea8e.s[63]++;
      saveToFile(output, results);

      /* istanbul ignore next */
      cov_nmtawea8e.s[64]++;
      openInBrowser(open, results);

      /* istanbul ignore next */
      cov_nmtawea8e.s[65]++;

      if (returnHtmlBody) {
        /* istanbul ignore next */
        cov_nmtawea8e.b[13][0]++;
        cov_nmtawea8e.s[66]++;
        return resolve({
          results,
          body
        });
      } else
      /* istanbul ignore next */
      {
        cov_nmtawea8e.b[13][1]++;
      }

      cov_nmtawea8e.s[67]++;
      return resolve(results);
    }).catch(reject);
  });
}

