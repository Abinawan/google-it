{
  "name": "google-it",
  "version": "1.4.0",
  "description": "A CLI and Node.js library to help retrieve, display, and store Google search results",
  "main": "./src/googleIt.js",
  "bin": "./src/app.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/PatNeedham/google-it.git"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js --ignore-path .eslintignore '*.js'",
    "test": "jest"
  },
  "keywords": [
    "google-search",
    "google-search-to-json"
  ],
  "author": "Pat Needham (pat@needham.io)",
  "license": "ISC",
  "jest": {
    "preset": "amex-jest-preset",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.4.0",
    "command-line-args": "^5.0.0",
    "ora": "^4.0.3",
    "request": "^2.88.0"
  },
  "devDependencies": {
    "amex-jest-preset": "^5.0.0",
    "babel-preset-amex": "^3.3.0",
    "codecov": "^3.6.1",
    "eslint": "^5.16.0",
    "eslint-config-amex": "^10.1.0",
    "jest": "^24.9.0",
    "mocha": "^6.2.2",
    "prettier": "1.19.1"
  }
}
